#include<stdio.h>
#include<ctype.h>
int main()
{
    char ctr;
    scanf("%c",&ctr);
    if(isalnum(ctr)!=0)
    {
        if (isalpha(ctr) == 0)
            printf("This is a number.");
        else
         printf("This is an alphabet.");    
    }
    else
        printf("This is a special character.");
}
